{
  "name": "jinn-node",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/worker/mech_worker.js",
  "types": "dist/worker/mech_worker.d.ts",
  "bin": {
    "jinn-worker": "./dist/worker/mech_worker.js"
  },
  "exports": {
    "./worker": "./dist/worker/mech_worker.js",
    "./worker/*": "./dist/worker/*.js",
    "./agent": "./dist/agent/agent.js",
    "./agent/*": "./dist/agent/*.js",
    "./logging": "./dist/logging/index.js",
    "./config": "./dist/config/index.js",
    "./env": "./dist/env/index.js",
    "./env/*": "./dist/env/*.js",
    "./http": "./dist/http/client.js",
    "./shared": "./dist/shared/index.js",
    "./shared/*": "./dist/shared/*.js",
    "./types": "./dist/types/index.js"
  },
  "files": [
    "dist",
    "abis",
    "scripts",
    ".env.example"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "dev": "tsx src/worker/mech_worker.ts",
    "dev:mech": "sh -c 'USE_TSX_MCP=1 tsx src/worker/mech_worker.ts \"$@\" | tsx src/logging/dev-pretty-cli.ts' --",
    "mcp:start": "tsx --env-file=../.env src/agent/mcp/server.ts",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@jinn-network/mech-client-ts": "^0.0.6",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@octokit/rest": "^22.0.1",
    "@safe-global/protocol-kit": "^6.1.0",
    "@supabase/supabase-js": "^2.53.0",
    "civitai": "^0.1.15",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0",
    "graphql-request": "^7.2.0",
    "openai": "^5.11.0",
    "pg": "^8.12.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "viem": "^2.35.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@google/gemini-cli": "^0.25.0",
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.33",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
