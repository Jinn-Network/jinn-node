{
  "name": "jinn-node",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/worker/mech_worker.js",
  "types": "dist/worker/mech_worker.d.ts",
  "bin": {
    "jinn-worker": "./dist/worker/mech_worker.js",
    "jinn-setup": "./dist/setup/cli.js"
  },
  "exports": {
    "./worker": "./dist/worker/mech_worker.js",
    "./worker/*": "./dist/worker/*.js",
    "./worker/git/*": "./dist/worker/git/*.js",
    "./worker/mcp/*": "./dist/worker/mcp/*.js",
    "./worker/mcp/tools/*": "./dist/worker/mcp/tools/*.js",
    "./worker/prompt/*": "./dist/worker/prompt/*.js",
    "./worker/prompt/utils/*": "./dist/worker/prompt/utils/*.js",
    "./worker/prompt/providers/*": "./dist/worker/prompt/providers/*.js",
    "./worker/prompt/providers/context/*": "./dist/worker/prompt/providers/context/*.js",
    "./worker/prompt/providers/invariants/*": "./dist/worker/prompt/providers/invariants/*.js",
    "./worker/delivery/*": "./dist/worker/delivery/*.js",
    "./worker/execution/*": "./dist/worker/execution/*.js",
    "./worker/metadata/*": "./dist/worker/metadata/*.js",
    "./worker/status/*": "./dist/worker/status/*.js",
    "./worker/logging/*": "./dist/worker/logging/*.js",
    "./worker/contracts/*": "./dist/worker/contracts/*.js",
    "./worker/config/*": "./dist/worker/config/*.js",
    "./worker/types/*": "./dist/worker/types/*.js",
    "./worker/recognition/*": "./dist/worker/recognition/*.js",
    "./worker/reflection/*": "./dist/worker/reflection/*.js",
    "./worker/orchestration/*": "./dist/worker/orchestration/*.js",
    "./worker/llm/*": "./dist/worker/llm/*.js",
    "./worker/filters/*": "./dist/worker/filters/*.js",
    "./agent": "./dist/agent/agent.js",
    "./agent/*": "./dist/agent/*.js",
    "./agent/shared/*": "./dist/agent/shared/*.js",
    "./agent/mcp/*": "./dist/agent/mcp/*.js",
    "./agent/mcp/tools/*": "./dist/agent/mcp/tools/*.js",
    "./agent/mcp/tools/shared/*": "./dist/agent/mcp/tools/shared/*.js",
    "./logging": "./dist/logging/index.js",
    "./logging/*": "./dist/logging/*.js",
    "./config": "./dist/config/index.js",
    "./config/*": "./dist/config/*.js",
    "./env": "./dist/env/index.js",
    "./env/*": "./dist/env/*.js",
    "./http": "./dist/http/client.js",
    "./http/*": "./dist/http/*.js",
    "./shared": "./dist/shared/index.js",
    "./shared/*": "./dist/shared/*.js",
    "./data/*": "./dist/data/*.js",
    "./lib/*": "./dist/lib/*.js",
    "./types": "./dist/types/index.js",
    "./types/*": "./dist/types/*.js",
    "./setup": "./dist/setup/index.js",
    "./setup/*": "./dist/setup/*.js"
  },
  "files": [
    "dist",
    "scripts",
    ".env.example",
    "yarn.lock",
    "pyproject.toml",
    "poetry.lock"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.build.json && copyfiles -u 1 'src/**/*.json' 'src/gemini-extension/**/*.md' dist",
    "worker": "sh -c 'LOG_PRETTY=1 USE_TSX_MCP=1 tsx src/worker/mech_worker.ts \"$@\"' --",
    "setup": "sh -c 'LOG_PRETTY=1 tsx src/setup/cli.ts \"$@\"' --",
    "typecheck": "tsc --noEmit",
    "wallet:info": "tsx scripts/wallet/info.ts",
    "wallet:backup": "tsx scripts/wallet/backup.ts",
    "wallet:export-keys": "tsx scripts/wallet/export-keys.ts",
    "wallet:withdraw": "sh -c 'tsx scripts/wallet/withdraw.ts \"$@\"' --",
    "wallet:unstake": "sh -c 'tsx scripts/wallet/unstake.ts \"$@\"' --",
    "wallet:recover": "sh -c 'tsx scripts/wallet/recover.ts \"$@\"' --",
    "staking:claim-incentives": "sh -c 'tsx scripts/staking/claim-incentives.ts \"$@\"' --",
    "staking:claim-rewards": "sh -c 'tsx scripts/staking/claim-service-rewards.ts \"$@\"' --",
    "support:bundle": "FORCE_STDERR=true tsx scripts/support-bundle.ts"
  },
  "dependencies": {
    "@jinn-network/mech-client-ts": "^0.0.6",
    "@modelcontextprotocol/sdk": "^1.11.0",
    "@octokit/rest": "^22.0.1",
    "@safe-global/protocol-kit": "^6.1.0",
    "@slicekit/erc8128": "^0.1.0",
    "@supabase/supabase-js": "^2.53.0",
    "civitai": "^0.1.15",
    "cross-fetch": "^4.1.0",
    "dotenv": "^17.2.2",
    "ethers": "^6.15.0",
    "google-auth-library": "^9.15.1",
    "graphql-request": "^7.2.0",
    "openai": "^5.11.0",
    "pg": "^8.12.0",
    "pino": "^9.9.0",
    "pino-pretty": "^13.1.1",
    "viem": "^2.35.1",
    "yargs": "^17.7.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@google/gemini-cli": "^0.25.0",
    "@types/node": "^20.0.0",
    "@types/yargs": "^17.0.33",
    "copyfiles": "^2.4.1",
    "tsx": "^4.20.6",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "yarn@1.22.22"
}
